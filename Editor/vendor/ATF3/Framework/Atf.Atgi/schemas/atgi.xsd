							<xs:complexType>
								<xs:simpleContent>
									<xs:extension base="floatListType">
										<xs:attribute name="channels" type="jointFreedomsType" />
									</xs:extension>
								</xs:simpleContent>
							</xs:complexType>
						</xs:element>
						<xs:element name="maxrotation" minOccurs="0" maxOccurs="1">
							<xs:complexType>
								<xs:simpleContent>
									<xs:extension base="floatListType">
