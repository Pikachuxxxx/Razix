	<!-- Joint. -->
	<xs:complexType name="jointType">
		<xs:complexContent>
			<xs:extension base="nodeType">
				<xs:sequence>
					<xs:sequence>
						<xs:element name="freedoms" minOccurs="1" maxOccurs="1">
