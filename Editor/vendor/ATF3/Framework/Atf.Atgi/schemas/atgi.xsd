						<xs:element name="up" type="vector3Type" minOccurs="1" maxOccurs="1"/>
						<xs:element name="globalup" type="vector3Type" minOccurs="0" maxOccurs="1"/>
						<xs:element name="upobject" minOccurs="0" maxOccurs="1">
							<xs:complexType>
								<xs:attribute name="name" type="aifPathType"/>
								<xs:attribute name="transform">
