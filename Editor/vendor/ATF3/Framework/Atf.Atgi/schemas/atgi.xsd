		</xs:sequence>
	</xs:complexType>
	<!-- Expression type. -->
	<xs:complexType name="expressionType">
		<xs:complexContent>
			<xs:extension base="objType">
				<xs:sequence>
					<xs:sequence>
						<xs:element name="input" minOccurs="0" maxOccurs="unbounded">
